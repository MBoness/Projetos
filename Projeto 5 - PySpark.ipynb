   "source": [
    "credit_currency = spark.sql('''\n",
    "SELECT\n",
    "    SK_ID_CURR,\n",
    "    SUM(CASE WHEN CREDIT_CURRENCY = 'currency 1' THEN 1 ELSE 0 END) AS credit_currency_1,\n",
    "    SUM(CASE WHEN CREDIT_CURRENCY <>'currency 1' THEN 1 ELSE 0 END) AS credit_currency_other\n",
    "FROM bureau\n",
    "GROUP BY SK_ID_CURR\n",
    "''')"
